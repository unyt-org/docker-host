### bubbleSort ###
bubbleSort = fun (<Array> a) => (
    n = count a;
    lbl repeat
        swapped = false;
        i = 1;
        while (i < n) (
            x = a.(i);
            y = a.(i - 1);
            if (y > x) (
                a.(i - 1) = x;
                a.(i) = y;
                swapped = true;
            );
            i += 1;
        );
    jtr repeat swapped;
    a;
);

